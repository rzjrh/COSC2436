
public class Test1 {


	public static void main(String[] args) {

		double [][] m = {{1,2,3},{1.5,25,3.5},{0.1,0.1,0.1}};
		System.out.println(sum(m));
		
	}

	public static double sum (double [][] m){
		double sum = 0;
	    for (int i=0; i<m.length; i++)
	    sum += m[i][i];
	    return sum;
	}	
	
}
